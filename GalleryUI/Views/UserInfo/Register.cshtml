
@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Register-美图网</title>
    <link rel="stylesheet" href="/content/login/normalize.css">
    <link rel="stylesheet" href="/content/login/style.css" media="screen" type="text/css" />
    <script src="~/Scripts/formatVerify.js"></script>
    <script src="~/Scripts/md5.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/layer9.1/layer.js"></script>
    <script src="~/Scripts/loginRegister.js"></script>
    <script src="~/Scripts/jquery.fancybox.js"></script>
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />
    <script type="text/javascript">
        //异步加载的提示
        $(document).ajaxStart(function () {
            $.fancybox.showLoading();
        })
        $(document).ajaxStop(function () {
            $.fancybox.hideLoading();
        })
    </script>
</head>
<body>
    
    <section class="login-form-wrap">
        <h1>美图网</h1>
        <form class="login-form">
            <label>
                <input type="email" id="email" required placeholder="Email">
            </label>
            <label>
                <input type="password" id="pwd" required placeholder="Password">
            </label>
            <input type="submit" value="Register" onclick="register()">
        </form>
        <h5><a href="/UserInfo/Login" style="color:#ff5600;font-size:large">Login</a></h5>
    </section>

</body>
</html>


